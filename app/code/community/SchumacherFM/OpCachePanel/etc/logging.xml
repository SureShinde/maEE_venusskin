<logging>
    <opcache translate="label">
        <label>Manage OpCache Keys</label>
        <expected_models>
            <opcache__banner/>
        </expected_models>
        <actions>
            <adminhtml_opcache_edit>
                <action>view</action>
            </adminhtml_opcache_edit>

            <adminhtml_opcache_save>
                <action>save</action>
                <skip_on_back>
                    <adminhtml_opcache_edit/>
                </skip_on_back>
            </adminhtml_opcache_save>

            <adminhtml_opcache_delete>
                <action>delete</action>
            </adminhtml_opcache_delete>

            <adminhtml_opcache_massDelete>
                <action>massDelete</action>
            </adminhtml_opcache_massDelete>
        </actions>
    </opcache>
